{
  "name": "website-next-v2",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "export": "next build && next export",
    "heroku-start": "NODE_ENV=production node server.js -p $PORT",
    "heroku-postbuild": "yarn build",
    "sync:aws:next:staging": "aws s3 sync .next s3://aimementoring-staging/website/public/_next --acl public-read --delete",
    "sync:aws:next:master": "aws s3 sync .next s3://aimementoring/website/public/_next --acl public-read --delete",
    "budget": "lighthouse --budget-path=./lighthouse-budget.json --output html --output-path ./performance-artifacts/budget-report.html --save-assets --view",
    "audit": "lighthouse --config-path=lighthouse-config.js --output html --output-path ./performance-artifacts/audit-report.html --save-assets --view",
    "lhbot": "lighthousebot -- --pwa=95 --perf=95 --seo=95 --a11y=95 --bp=95",
    "gimbal-audit": "gimbal audit --config ./.gimbalrc.yml",
    "precommit": "lint-staged",
    "lint": "eslint .",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "cli-login": "contentful login --management-token",
    "cli-logout": "contentful logout",
    "cli-migrate": "contentful space migration --environment-id 'staging'",
    "cli-space-id": "contentful space use --space-id",
    "cli-set-env": "contentful space environment use --environment-id"
  },
  "dependencies": {
    "@bugsnag/js": "^6.4.3",
    "@bugsnag/plugin-react": "^6.4.0",
    "@modus/gimbal-plugin-axe": "^1.2.6",
    "@modus/gimbal-plugin-last-value": "^1.2.6",
    "@modus/gimbal-plugin-sqlite": "^1.2.6",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "aime-blueprint": "^2.2.3",
    "airtable": "^0.7.1",
    "axios": "^0.19.0",
    "cacheable-response": "^1.7.4",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "contentful": "^7.10.0",
    "countries": "^0.1.1",
    "country-list": "^2.2.0",
    "cross-fetch": "^3.0.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "filestack-react": "^3.1.0",
    "google-maps": "^3.3.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mux.js": "^5.5.1",
    "next": "9.1.1",
    "next-compose-plugins": "^2.2.0",
    "next-offline": "^4.0.6",
    "node-sass": "^4.13.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.0",
    "rc-select": "8.8.4",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-gtm-module": "^2.0.8",
    "react-helmet": "^5.2.1",
    "react-html-parser": "^2.0.2",
    "react-intl-tel-input": "^7.1.0",
    "react-markdown": "^4.2.2",
    "react-modal": "^3.11.1",
    "react-places-autocomplete": "^7.2.1",
    "react-scroll-to-component": "^1.0.2",
    "react-select": "^3.0.8",
    "react-slick": "^0.25.2",
    "shopify-buy": "^2.8.1",
    "slick-carousel": "^1.8.1",
    "spdy": "^4.0.1",
    "video.js": "7.0.5",
    "videojs-contrib-hls": "5.14.1"
  },
  "devDependencies": {
    "@modus/gimbal": "^1.2.6",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.9",
    "lighthouse": "^5.4.0",
    "lint-staged": "^10.0.0-1"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint --ext .js"
  }
}
